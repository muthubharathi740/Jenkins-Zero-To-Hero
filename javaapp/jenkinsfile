pipeline {
  agent none
  stages {
    stage('Back-end') {
      agent {
        docker { image 'maven:3.8.1-adoptopenjdk-11' }
      }
      steps {
        public class HelloWorld {
    public static void main(String[] args) {
        // Write your code here
    }
}
    } 
    stage('Front-end') {
      agent {
        docker { image 'node:16-alpine' }
      }
      steps {
        sh 'console.log("Hello World");'
      }
    }
  }
}
}
